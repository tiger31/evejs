{
  "name": "mocha-api-integration",
  "version": "1.0.4",
  "description": "Mocha extension for integration api testing",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/_mocha --require test/utils.js --recursive \"test/**/*.test.js\"",
    "test:coverage": "NODE_ENV=test node_modules/.bin/nyc --reporter=lcovonly ./node_modules/mocha/bin/_mocha --require test/utils.js --recursive \"test/**/*.test.js\" && node_modules/.bin/codecov",
    "eslint": "./node_modules/.bin/eslint lib/classes",
    "eslint-fix": "./node_modules/.bin/eslint lib/classes --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/IllidariCat/mocha-integration.git"
  },
  "author": "IllidariCat",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/IllidariCat/mocha-integration/issues"
  },
  "homepage": "https://gitlab.com/IllidariCat/mocha-integration#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "bluebird": "^3.7.2",
    "callsite-record": "^4.1.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^3.0.0",
    "glob": "^7.1.6",
    "mocha": "^6.1.4",
    "mocha-steps": "^1.3.0",
    "moment": "^2.24.0",
    "socket.io-client": "^2.3.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/register": "^7.7.4",
    "@babel/runtime": "^7.7.6",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^5.2.0",
    "codecov": "^3.6.1",
    "eslint": "^6.6.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jsdoc": "^18.0.1",
    "nyc": "^14.1.1"
  }
}
